													<span class="meta_item meta_tag">
														{% set separator = '' %}
														{% for tag in blogpost.tags %}
														{{separator}}<a class=" " href="{{ path('list_index', {pattern: tag}) }}">{{ tag }}</a>
														{% set separator = ', ' %}
														{% endfor %}
													</span>
												</p>
											</div>
										</div>
										<div class="post_content">
											<div class="post_c_in">
												{% for blogcontent in blogpost.content %}
													{% if blogcontent.html != '' %}
														{{ blogcontent.html|raw }}
													{% endif %}
													{% if blogcontent.image != '' %}
														<figure class="alignleft"><img src="{{ blogcontent.image }}" alt="">
														</figure>
													{% endif %}
													{% if blogcontent.youtube != '' and blogcontent.youtube != blogpost.content[0].youtube %}
														{{ blogcontent.youtube }}
													{% endif %}
												{% endfor %}

												<div class="clearfix"></div>
											</div>
										</div>
